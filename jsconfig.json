{
  "compilerOptions": {
    "rootDir": ".",
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@comcom/*": [
        "src/components/common/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
